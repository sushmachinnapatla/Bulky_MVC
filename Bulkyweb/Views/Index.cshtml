@model List<Category>
<h1> Category List</h1>
<table class="table-bordered table-stripped">
    <thread>
    <tr>
        <th>
            Category Name 

        </th>
        <th>
            Display Order
        </th>
    </tr>
    </thread>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder))
        {
            <tr>
                <td>
                    @obj.Name
                </td>
                <td>DisplayOrder</td>
            </tr>
            
        }
    </tbody>

</table>
